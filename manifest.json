{
	"manifest_version": 2,
	"name": "Tile Enhancer",
	"version": "0.2.0",
	"description": "An unofficial tool that allows the user to enhance the satellite images on the GlobalXplorerÂ° website",
	"author": "ppseprus",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.globalxplorer.org/*"
			],
			"css": [
				"styles.css"
			],
			"js": [
				"enhance.js"
			],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"webNavigation",
		"tabs"
	]
}
