{
	"manifest_version": 2,
	"name": "GlobalXplorer° Tile Enhancer",
	"short_name": "Tile Enhancer",
	"version": "0.1.4",
	"description": "A tool that allows the user to enhance the satellite images on the GlobalXplorer° website",
	"author": "ppseprus",
	"icons": {
		"16": "img/gx16.png",
		"32": "img/gx32.png",
		"96": "img/gx96.png",
		"128": "img/gx128transparent.png",
		"192": "img/gx192.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.globalxplorer.org/explore"
			],
			"js": [
				"enhance.js"
			],
			"run_at": "document_end"
		}
	]
}
